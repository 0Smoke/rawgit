{
    "name"   : "rawgit",
    "version": "0.0.1",
    "private": true,

    "engines": {
        "node": ">=0.10"
    },

    "dependencies": {
        "express"           : "4.12.3",
        "express-handlebars": "2.0.1",
        "lru-cache"         : "2.5.0",
        "mime-db"           : "1.8.0",
        "morgan"            : "1.5.2",
        "newrelic"          : "1.19.1",
        "on-finished"       : "2.2.0"
    },

    "devDependencies": {
        "chai"     : "^2.2.0",
        "mocha"    : "^2.2.1",
        "nock"     : "^1.3.0",
        "supertest": "0.15.0"
    },

    "scripts": {
        "clean-fixtures": "scripts/clean-fixtures.js test/fixtures/*.json",
        "record-fixtures": "NOCK_BACK_MODE=record npm test && npm run-script clean-fixtures",
        "start": "node web.js",
        "test": "NODE_ENV=test mocha -R dot test/**/test.*.js"
    }
}
